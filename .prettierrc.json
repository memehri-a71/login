{
  "printWidth": 80,
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "importOrder": [
    "^stylis$",
    "^jalali-moment$",
    "^stylis-plugin-rtl$",
    "^@emotion/(.*)$",
    "^next/(.*)$",
    "^next-intl/(.*)$",
    "^formik$",
    "^formik/(.*)$",
    "^@tanstack/(.*)$",
    "^@tanstack/query-core$",
    "^@mui/(.*)$",
    "^next-intl$",
    "^ui$",
    "^@/types/(.*)$",
    "^@types/(.*)$",
    "^@/views/(.*)$",
    "^@views/(.*)$",
    "^views/(.*)$",
    "^@views$",
    "^@/views$",
    "^@/hooks/(.*)$",
    "^@hooks/(.*)$",
    "^hooks/(.*)$",
    "^@hooks$",
    "^@/hooks$",
    "^@/constants/(.*)$",
    "^@constants/(.*)$",
    "^constants/(.*)$",
    "^@constants$",
    "^@/constants$",
    "^@/configs/(.*)$",
    "^@configs/(.*)$",
    "^configs/(.*)$",
    "^@configs$",
    "^@/configs$",
    "^@/components/(.*)$",
    "^@components/(.*)$",
    "^components/(.*)$",
    "^@components$",
    "^@/components$",
    "^@/types$",
    "^@types$",
    "^types/(.*)$",
    "^@ui/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "^.*.css$",
    "^@/utils/(.*)$"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
